    <h1 class="user-id" style="display: none"><%= users._id%></h1>
        <h1 class="tkn" style="display: none"><%= users.token%></h1>
        <img src="" alt="caca" class="cover">
    </div>
<script src="/scripts/connected.js"></script>
<script>
    if('serviceWorker' in navigator) {
        navigator.serviceWorker
            .register('/sw.js')
            .then(function() { console.log("Service Worker Registered"); });
    }
</script>
    <script>
        var idr = document.querySelector('.user-id').innerHTML;
        var tkn = document.querySelector('.tkn').innerHTML;
        var urls = 'users/' + idr;

        var xhtttpx = new XMLHttpRequest();
        xhtttpx.onreadystatechange = function() {
            if (this.readyState === 4 && this.status === 200) {
                let userData= JSON.parse(this.responseText);
                var xhtttpxx = new XMLHttpRequest();
                xhtttpxx.onreadystatechange = function() {
                    if (this.readyState === 4 && this.status === 200) {
                        let data = JSON.parse(this.responseText)
                        console.log(data.cover.source);
                        fetch(urls, {
                            method: 'put',
                            headers: {'Content-Type': 'application/json'},
                            body: JSON.stringify({
                                'friends': data.cover.source
                            })
                        })
                            .then(function(){
                                var xhtttpxdx = new XMLHttpRequest();
                                xhtttpxdx.onreadystatechange = function() {
                                    if (this.readyState === 4 && this.status === 200) {
                                        let dataf = JSON.parse(this.responseText);
                                        document.querySelector('.cover').src = dataf.friends;
                                    }
                                }
                                xhtttpxdx.open("GET",urls, true);
                                xhtttpxdx.send();
                            })
                    }
                }
                xhtttpxx.open("GET","https://graph.facebook.com/" + userData.id + "?fields=cover&access_token=" + tkn , true);
                xhtttpxx.send();
            }
        };
        xhtttpx.open("GET",urls, true);
        xhtttpx.send();
    </script>
